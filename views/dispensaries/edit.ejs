<% include ../partials/header %>


<div class="container">
  <div class="col-md-4 col-md-offset-4 col-xs-10 col-xs-offset-1">
    <h1 style="text-align: center;">Edit Dispensary: <%= dispensary.name %></h1>
    <div class="well login-well">
      <form action="/dispensaries/<%= dispensary._id %>?_method=PUT" method="POST">
        <div class="form-group">
          <input class="form-control" type="text" name="name" placeholder="Name" autocomplete="off" value="<%= dispensary.name %>">
        </div>
        <div class="form-group">
          <input class="form-control" type="text" name="location" placeholder="Location" value="<%= dispensary.location %>">
        </div>
        <div class="form-group">
          <input class="form-control" type="text" name="description" placeholder="Description" value="<%= dispensary.description %>">
        </div>
        <div class="form-group">
          <input class="form-control" type="text" name="image" placeholder="Image URL" value="<%= dispensary.image %>">
        </div>
        <div class="form-group">
          <button class="btn btn-lg btn-primary btn-block">Add</button>
        </div>
      </form>
    </div>
  </div>
</div>

<% include ../partials/footer %>